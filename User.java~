import java.util.*;
public class User {
	
	private String username;
	private String password;
	private int uid;
	
	public void setUsername(String x)
	{
		username = x;
	}
	
	public String getUsername()
	{
		return username;
	}
	
	public void setPassword(String y)
	{
		password = y;
	}
	
	public String getPassword()
	{
		return password;
	}

	public void setId(int i)
	{
		uid = i;
	}
	
	public int getId()
	{
		return uid;
	}
	
	
	
}

